<h5>Editing patient</h5>

<%= form_for(@patient) do |f| %>
  <%= render '/shared/errors', model: @patient %>

  <%= render :partial => 'patient_form',      :locals => {:f => f, :patient_id => @patient.id} %>

  <%= f.submit "Update", class: "btn btn-primary" -%>&nbsp;&nbsp;<%= link_to 'Cancel', :back %>
<% end %>
