<h5>Update alignment reference</h5>

<%= form_for(@alignment_ref) do |f| %>
    <%= render '/shared/errors', model: @alignment_ref %>

    <%= render :partial => 'alignment_ref', :locals => {:f => f} %>

    <%= f.submit "Update", class: "btn btn-primary" %>
<% end %>

<%= link_to 'List All', alignment_refs_path %>