<h5><%=h @hdr %></h5>

<% if @hdr == 'Sequencing Runs' %>
  <%= form_tag url_for(:controller => :flowcell_queries, :action => :export_seqruns) do %>
    <%= submit_tag "Export Seq Runs", class: "btn btn-primary" -%>
    <br/><br/>
    <%= render :partial => 'flow_cells_list', :locals => {:fform => 'yes'} %>
  <% end %>

<% else %>
  <%= render :partial => 'flow_cells_list', :locals => {:fform => 'yes'} %>
<% end %>