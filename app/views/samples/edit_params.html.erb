<div class="card">
  <h6 class="card-header">Select sample to edit</h6>
  <div class="card-body">


<%= form_tag :action => 'edit_params' do %>

  <div class="form-group row">
    <label for="barcode_key" class="col-auto col-form-label col-form-label-sm">Barcode:</label>
    <div class="col-auto">
      <%= text_field_tag :barcode_key, nil, size: 12, placeholder: "Barcode", class: "form-control" %>
    </div>
  </div>

  <%= submit_tag "Submit", class: "btn btn-primary" %>
<%end%>

  </div>
</div>
